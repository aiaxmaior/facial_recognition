{
    "device_id": "jetson-001",
    "broker_url": "https://acetaxi-bridge.qryde.net/iot-broker",
    "api_key": null,
    "endpoints": {
        "events": "/events",
        "heartbeat": "/heartbeat",
        "registration": "/registration",
        "media": "/media"
    },
    "camera": {
        "rtsp_url": "rtsp://admin:Fanatec2025@192.168.50.141/Preview_01_main",
        "fps": 25
    },
    "pipeline": {
        "mode": "deepstream",
        "deepstream": {
            "latency_ms": 100,
            "drop_on_latency": true,
            "output_width": 640,
            "output_height": 360,
            "output_format": "BGRx"
        },
        "fallback_mode": "opencv"
    },
    "detection": {
        "use_tensorrt": true,
        "engine_path": "models/yolov8_face/yolov8n-face.engine",
        "input_size": 640,
        "conf_threshold": 0.5,
        "nms_threshold": 0.45
    },
    "recognition": {
        "model": "ArcFace",
        "detector_backend": "yolov8n",
        "distance_threshold": 0.55,
        "min_face_size": 25,
        "min_confidence": 0.5,
        "process_fps": 5,
        "process_width": 640,
        "detection_width": 2560
    },
    "validation": {
        "confirmation_frames": 3,
        "consistency_threshold": 0.6,
        "cooldown_seconds": 3
    },
    "video_buffer": {
        "enabled": false,
        "duration_seconds": 8,
        "buffer_path": "/tmp/video_buffer",
        "pre_event_seconds": 5,
        "post_event_seconds": 3
    },
    "sync": {
        "enrollment_db_path": "data/enrollments.db",
        "socketio_url": "https://acetaxi-bridge.qryde.net",
        "socketio_path": "/iot-broker/api/socket.io"
    },
    "heartbeat": {
        "interval_seconds": 30
    }
}
